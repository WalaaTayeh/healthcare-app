
{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "client/public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "env": {
    "NODE_VERSION": "20"
}
}
